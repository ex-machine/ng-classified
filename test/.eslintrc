{
	"extends": [
		"eslint:recommended"
	],
	"rules": {
        "comma-dangle": [1, "never"],
        "no-console": 1,
		"no-empty": 1,
        "no-unused-vars": [1, {"vars": "all", "args": "none"}],
		"strict": [0, "global"]
	},
	"env": {
		"es6": true,
		"browser": true,
		"jasmine": true
	},
	"globals": {
		"exports": true,
		"module": false,
		"angular": false,
		"require": false,
		"inject": false
	}
}
